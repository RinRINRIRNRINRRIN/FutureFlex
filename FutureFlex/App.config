<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <add key="COM_WGH" value="COM6" />
    <add key="BAUDRATE" value="9600" />
    <add key="Parity" value="None" />
    <add key="StopBits" value="1" />
    <add key="DataBits" value="8" />


    <add key="SERVER_LOCAL" value="localhost" />
    <add key="PORT_LOCAL" value="1433" />
    <add key="USER_LOCAL" value="sa" />
    <add key="PASS_LOCAL" value="123456" />
    <add key="DB_LOCAL" value="FutureFlex" />

    <add key="DEVICE" value="DI81" />
  </appSettings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
